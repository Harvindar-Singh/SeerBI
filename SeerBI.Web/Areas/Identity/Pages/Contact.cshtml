@page
@model SeerBI.Web.Areas.Identity.Pages.ContactModel
@{
    ViewData["Title"] = "Contact page";
}
<h1>Contact us if you want our data science or machine learning facilities</h1>

@if(Model.hasData)
{
    <p>Hello @Model.firstname @Model.lastname</p>
    <p>We received your following message correctly:</p>
    <p>@Model.message</p>


}
<div>
   <form method="post">
        <label> First Name</label><br>
        <input type="text" id="fname" name="firstname"><br>

        <label>Last Name</label><br>
        <input type="text" id="lname" name="lastname"><br>

         <label>Message</label><br>
         <textarea id="message" name="message" style=" height:200px"></textarea><br>

         <input type="submit" value=" Submit">
   </form>

</div>